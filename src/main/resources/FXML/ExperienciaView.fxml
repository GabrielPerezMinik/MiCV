<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="experienciaView" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView fx:id="experienciaTableView" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="desdeColumtable" prefWidth="91.0" text="Desde" />
          <TableColumn fx:id="hastaColumntable" prefWidth="73.0" text="Hasta" />
            <TableColumn fx:id="denominacioColumnTable" prefWidth="91.0" text="Denominación" />
            <TableColumn fx:id="empleadorColumTable" prefWidth="82.0" text="Empleador" />
        </columns>
      </TableView>
   </center>
   <right>
      <VBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onAnadir" text="Añadir " />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onElimiar" text="Eliminar" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="2.0" />
         </padding>
      </VBox>
   </right>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</BorderPane>
